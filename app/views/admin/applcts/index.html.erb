<p id="notice"><%= notice %></p>

<h1>Listing Admin Applcts</h1>

<table>
  <thead>
    <tr>
      <th>Identity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_applcts.each do |admin_applct| %>
      <tr>
        <td><%= admin_applct.identity %></td>
        <td><%= link_to 'Show', admin_applct %></td>
        <td><%= link_to 'Edit', edit_admin_applct_path(admin_applct) %></td>
        <td><%= link_to 'Destroy', admin_applct, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Applct', new_admin_applct_path %>
